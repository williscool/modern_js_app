{
  "extends": ["airbnb", "prettier"],
  "env": {
    "jest": true,
    "browser": true
  },
  "rules": {
    "max-len": ["error", 120, { "ignoreComments": true }],
    "valid-jsdoc": ["error", { "requireReturn": false }],
    "linebreak-style": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
