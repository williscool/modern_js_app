{
  "name": "vanilla-js-photos",
  "version": "0.0.1-dev",
  "description": "Modern JS app for GDAX Exchange Quotes",
  "main": "index.js",
  "repository": "[none]",
  "author": "William Harris <wharris@upscalews.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn dev:wds",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf dist && NODE_ENV=production webpack -p --progress",
    "lint":
      "prettier --write . && eslint src webpack.config.babel.js --ext .js && tslint -p . -c tslint.json && ./node_modules/stylelint/bin/stylelint.js 'src/**/*.css'",
    "test": "yarn lint && jest --coverage",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.4",
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.0",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "nightmare": "^2.10.0",
    "postcss": "^6.0.14",
    "postcss-cssnext": "^3.0.2",
    "postcss-hexrgba": "^1.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "precss": "^2.0.0",
    "prettier": "1.8.2",
    "react-addons-test-utils": "^15.6.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-config-prettier": "^1.6.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "@types/jest": "^21.1.6",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "awesome-typescript-loader": "^3.4.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "source-map-loader": "^0.2.3",
    "url-search-params-polyfill": "^2.0.1"
  },
  "browserslist": ["> 1%", "not ie <= 11", "safari >= 10", "ios >= 8"],
  "jest": {
    "testRegex": ".*-test\\.js$",
    "testEnvironment": "node"
  }
}
